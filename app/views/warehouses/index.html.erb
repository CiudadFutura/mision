<div class="mail">
  <h3>Puntos de retiro</h3>
  <div class="agileinfo_mail_grids">
    <div class="agileinfo_mail_grid_left">
    <% @warehouses.each do |warehouse| %>
      <% if ![2,4,7].include?(warehouse.id)%>
      <div class="row">
        <div class="col-md-12 warehouse">
          <h4><%= warehouse.name %></h4>
          <p><%= warehouse.description %></p>
          <ul>
            <li>
              <i class="fa fa-home" aria-hidden="true"></i>
            </li>
            <li>
              domicilio<span><%= warehouse.address %></span>
            </li>
          </ul>
          <ul>
            <li>
              <i class="fa fa-phone" aria-hidden="true"></i>
            </li>
            <li>
              whatsapp<span><%= warehouse.telephone %></span>
            </li>
          </ul>
          <ul>
            <li>
              <i class="fa fa-clock-o" aria-hidden="true"></i>
            </li>
            <li>
              horario de atenci√≥n<span><%= warehouse.working_hours %></span>
            </li>
          </ul>
        </div>
      </div>
      <% end %>
    <% end %>
    </div>
    <div class="clearfix"> </div>
  </div>
</div>

