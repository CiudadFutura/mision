<h1>Listing delivery_statuses</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @delivery_statuses.each do |delivery_status| %>
      <tr>
        <td><%= link_to 'Show', delivery_status %></td>
        <td><%= link_to 'Edit', edit_delivery_status_path(delivery_status) %></td>
        <td><%= link_to 'Destroy', delivery_status, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Delivery status', new_delivery_status_path %>
