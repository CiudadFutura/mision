<%- model_class = Pack -%>
<div class="container fill">
  <div class="row">

    <div id="js-message-success" style="display: none;">
      <div class="alert alert-success col-md-offset-3">
        <button type="button" class="close" data-dismiss="alert">&times;</button>
        Se agregó exitosamente el producto: <strong id="js-message-product"></strong>
      </div>
    </div>
    <div id="js-message-delete" style="display: none;">
      <div class="alert alert-danger col-md-offset-3">
        <button type="button" class="close" data-dismiss="alert">&times;</button>
        Se eliminó el producto del carrito</strong>
      </div>
    </div>
    <div class="row">
      <div class="products-search">
        <input type="text" id="products-search-txt" autofocus>
        <div class="results" id="products-search-results"></div>
      </div>
    </div>

    <div class="col-xs-12 col-md-9">

    </div> <!--col-xs-12 col-md-3-->
  </div> <!--close row -->
</div> <!--close containt fill-->

